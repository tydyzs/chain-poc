    <select id="selectTbCrdSumPage" resultMap="tbCrdSumResultMap">
        select * from TB_CRD_SUM where is_deleted = 0
    </select>

    <select id="selectTbCrdGuaranteeSumPage" resultType="org.git.modules.clm.credit.vo.CrdSumVO">
        select t1.*,t3.CUSTOMER_NAME,t3.CUSTOMER_TYPE,t4.CERT_TYPE,t4.CERT_NUM,t1.ORG_NUM as ORG_NUM_ANOTHER
        from tb_crd_sum t1
--         left join TB_CSM_MANAGE_TEAM t2 on t1.CUSTOMER_NUM = t2.CUSTOMER_NUM and t2.USER_PLACING_CD='01'
        left join TB_CSM_PARTY t3 on t1.CUSTOMER_NUM = t3.CUSTOMER_NUM
        left join TB_CSM_CERT_INFO t4 on t1.CUSTOMER_NUM = t4.CUSTOMER_NUM and t4.CERT_FLAG='01'
        left join CLM.CHAIN_DEPT t5 on t1.ORG_NUM = t5.ID
        where t1.CRD_PRODUCT_TYPE = '2'
        <if test="crdSumVO.userNum!=null and crdSumVO.userNum!=''">
            and t2.USER_NUM =#{crdSumVO.userNum}
        </if>
        <if test="crdSumVO.customerName!=null and crdSumVO.customerName!=''">
            and t3.CUSTOMER_NAME like CONCAT(CONCAT('%',#{crdSumVO.customerName}),'%')
        </if>
        <if test="crdSumVO.orgNum!=null and crdSumVO.orgNum!=''">
            and t5.ANCESTORS like  CONCAT(CONCAT('%',#{crdSumVO.orgNum}),'%')
        </if>
        <if test="crdSumVO.customerNum!=null and crdSumVO.customerNum!=''">
            and t1.customer_num =#{crdSumVO.customerNum}
        </if>
        <if test="crdSumVO.certNum!=null and crdSumVO.certNum!=''">
            and t4.cert_num =#{crdSumVO.certNum}
        </if>
        <if test="crdSumVO.certType!=null and crdSumVO.certType!=''">
            and t4.cert_type =#{crdSumVO.certType}
        </if>
    </select>

</mapper>
