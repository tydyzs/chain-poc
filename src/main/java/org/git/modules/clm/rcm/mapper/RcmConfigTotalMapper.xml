        <result column="QUOTA_NUM" property="quotaNum"/>
        <result column="QUOTA_USED_AMT" property="quotaUsedAmt"/>
        <result column="QUOTA_FREE_AMT" property="quotaFreeAmt"/>
        <result column="QUOTA_USED_RATIO" property="quotaUsedRatio"/>
