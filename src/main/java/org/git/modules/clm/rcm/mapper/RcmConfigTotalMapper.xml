        <result column="QUOTA_TOTAL_SUM" property="quotaTotalSum"/>
        <result column="LAST_YEAR_AMT" property="lastYearAmt"/>
        <result column="LAST_YEAR_RATIO" property="lastYearRatio"/>
        <result column="MONTH_TO_MONTH_AMT" property="monthToMonthAmt"/>
        <result column="MONTH_TO_MONTH_RATIO" property="monthToMonthRatio"/>
        <result column="HIS_FREQUENCY_A" property="hisFrequencyA"/>
        <result column="HIS_FREQUENCY_B" property="hisFrequencyB"/>
        <result column="HIS_FREQUENCY_C" property="hisFrequencyC"/>
        <result column="HIS_FREQUENCY" property="hisFrequency"/>
        <result column="HIS_FREQUENCY_A2" property="hisFrequencyA2"/>
        <result column="HIS_FREQUENCY_B2" property="hisFrequencyB2"/>
        <result column="HIS_FREQUENCY_C2" property="hisFrequencyC2"/>
        <result column="HIS_FREQUENCY_YEAR" property="hisFrequencyYear"/>
